		{
			"description": "Template parameters completions",
			"scope": "(text.html | source.wiki) - source - comment - string",
			"pattern": "^\\|([a-zA-Z0-9]*)",
			"completionCaptureIndex": 1,
			"completionSetNames": [
				"sfs.template-params"
			]
		},
		{
			"description": "Wiki Link completions",
			"scope": "(text.html | source.wiki) - source - comment - string",
			"pattern": "\\[\\[([a-zA-Z0-9:]*)",
			"completionCaptureIndex": 1,
			"completionSetNames": [
				"sfs.wiki-sharks",
				"sfs.wiki-categories",
				"sfs.wiki-categories-assignment",
				"sfs.wiki-links"
			]
		},
		{
			"description": "Chart Attribute Completion",
			"scope": "(text.html | source.wiki) - source - comment - string",
			"pattern": "<([a-zA-Z0-9]*)(?:\\s+[a-zA-Z0-9-:_]+\\s*=\\s*(?:'.*'|\".*\"))*(?:(?:\\s([a-zA-Z0-9-]+))|\\s)",
			"completionCaptureIndex": 2,
			"completionSetNames": [
				"sfs.$1.attr",
			]
		},
		{
			"description": "Chart Attribute Values Completion",
			"scope": "(text.html | source.wiki) meta.tag string.quoted",
			"pattern": "<([a-zA-Z0-9]*)(?:\\s+[a-zA-Z0-9-:_]+\\s*=\\s*(?:'.*'|\".*\"))*(?:\\s([a-zA-Z0-9-]+)\\s*=\\s*(?:[\"'])([a-zA-Z0-9-]*))",
			"completionCaptureIndex": 3,
			"completionSetNames": [
				"sfs.$1.$2.attr.values"
			]
		}
	]
